TYPE=$(busybox cat /sys/kernel/mcde/lcdclk | busybox grep -c "S6D")

if [ "$TYPE" == "1" ] ; then
	busybox echo lcdclk=30720000 > /sys/kernel/mcde/lcdclk
fi

busybox chown 1000:1000 /sys/kernel/god_mode/god_mode

busybox insmod /ramdisk/modules/j4fs.ko 
busybox insmod /ramdisk/modules/param.ko
busybox mkdir -p /mnt/.lfs
busybox mount -t j4fs /dev/block/mmcblk0p1 /mnt/.lfs
