on boot
    write /proc/sys/vm/page-cluster 0

on fs
    mount_all /fstab.samsungcodina
    start insert_modules

on property:sys.boot_completed=1
    stop auditd
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 200000

service insert_modules /system/bin/sh /ramdisk/insert_modules
    class late_start
    user root
    group root
    disabled
    oneshot

