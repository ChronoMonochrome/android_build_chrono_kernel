on boot
    write /proc/sys/vm/page-cluster 0

on fs:
    mount_all /fstab.samsungcodina
    start mount_modules

on property:sys.boot_completed=1
    stop auditd
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 200000

service mount_modules /system/bin/sh /ramdisk/mount_modules
    class late_start
    user root
    group root
    disabled
    oneshot

