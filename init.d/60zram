set -x

SIZE=256

if ! test -d /sys/module/vnswap; then
       insmod /system/lib/modules/vnswap.ko
fi

NUMBLOCKS=1

let "NUMBLOCKS=$NUMBLOCKS - 1"
for i in $(seq 0 "$NUMBLOCKS")
do
    echo $(( $SIZE * 1024 * 1024 )) > /sys/devices/virtual/block/vnswap$i/disksize 
    #echo 1 > /sys/devices/virtual/block/zram$i/max_comp_streams
    mkswap /dev/block/vnswap$i
    swapon /dev/block/vnswap$i
done
